<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dzp_temp_activity_mainlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/child_background"
    android:orientation="vertical" >

    <include layout="@layout/common_topbar" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:scrollbars="none" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/common_measure_1544dp"
                android:scaleType="fitCenter"
                android:src="@drawable/prize_region_bg" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/common_measure_1544dp"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/prize_region_temp_activity_first_text"
                    android:layout_width="@dimen/common_measure_100dp"
                    android:layout_height="@dimen/common_measure_37dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/common_measure_35dp"
                    android:layout_marginTop="@dimen/common_measure_430dp" />

                <Button
                    android:id="@+id/prize_region_temp_activity_first_btn"
                    android:layout_width="@dimen/common_measure_155dp"
                    android:layout_height="@dimen/common_measure_37dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/common_measure_55dp"
                    android:layout_marginTop="@dimen/common_measure_5dp"
                    android:background="@drawable/prize_region_btn_selector"
                    android:gravity="center"
                    android:text="领 取"
                    android:textColor="@color/prize_region_color2"
                    android:textSize="@dimen/common_measure_24dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/prize_region_temp_activity_sec_text"
                    android:layout_width="@dimen/common_measure_100dp"
                    android:layout_height="@dimen/common_measure_37dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/common_measure_35dp"
                    android:layout_marginTop="@dimen/common_measure_180dp" />

                <Button
                    android:id="@+id/prize_region_temp_activity_sec_btn"
                    android:layout_width="@dimen/common_measure_155dp"
                    android:layout_height="@dimen/common_measure_37dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/common_measure_55dp"
                    android:layout_marginTop="@dimen/common_measure_5dp"
                    android:background="@drawable/prize_region_btn_selector"
                    android:gravity="center"
                    android:text="领 取"
                    android:textColor="@color/prize_region_color2"
                    android:textSize="@dimen/common_measure_24dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/prize_region_temp_activity_third_text"
                    android:layout_width="@dimen/common_measure_100dp"
                    android:layout_height="@dimen/common_measure_37dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/common_measure_35dp"
                    android:layout_marginTop="@dimen/common_measure_158dp" />

                <Button
                    android:id="@+id/prize_region_temp_activity_third_btn"
                    android:layout_width="@dimen/common_measure_155dp"
                    android:layout_height="@dimen/common_measure_37dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/common_measure_55dp"
                    android:layout_marginTop="@dimen/common_measure_5dp"
                    android:background="@drawable/prize_region_btn_selector"
                    android:gravity="center"
                    android:text="领 取"
                    android:textColor="@color/prize_region_color2"
                    android:textSize="@dimen/common_measure_24dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/prize_region_temp_activity_fourth_text"
                    android:layout_width="@dimen/common_measure_100dp"
                    android:layout_height="@dimen/common_measure_37dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/common_measure_35dp"
                    android:layout_marginTop="@dimen/common_measure_180dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/common_measure_120dp"
                    android:layout_marginTop="@dimen/common_measure_130dp"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/prize_region_temp_activity_bottom_first_image"
                        android:layout_width="@dimen/common_measure_140dp"
                        android:layout_height="@dimen/common_measure_107dp"
                        android:background="@drawable/prize_region_first_image_start" />

                    <ImageView
                        android:id="@+id/prize_region_temp_activity_bottom_second_image"
                        android:layout_width="@dimen/common_measure_140dp"
                        android:layout_height="@dimen/common_measure_107dp"
                        android:layout_marginLeft="@dimen/common_measure_5dp"
                        android:layout_marginRight="@dimen/common_measure_5dp"
                        android:background="@drawable/prize_region_second_image_start" />

                    <ImageView
                        android:id="@+id/prize_region_temp_activity_bottom_third_image"
                        android:layout_width="@dimen/common_measure_140dp"
                        android:layout_height="@dimen/common_measure_107dp"
                        android:background="@drawable/prize_region_third_image_start" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <RelativeLayout
                    android:id="@+id/prize_region_temp_activity_rule_layout1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/common_measure_247dp"
                    android:layout_marginLeft="@dimen/common_measure_10dp"
                    android:layout_marginRight="@dimen/common_measure_10dp"
                    android:layout_marginTop="@dimen/common_measure_285dp"
                    android:background="@drawable/style_rect_fillet_fulling_deepblue"
                    android:visibility="invisible" >

                    <RelativeLayout
                        android:id="@+id/prize_region_temp_activity_rule1_delbtn1"
                        android:layout_width="@dimen/common_measure_60dp"
                        android:layout_height="@dimen/common_measure_60dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true" >

                        <ImageView
                            android:layout_width="@dimen/common_measure_25dp"
                            android:layout_height="@dimen/common_measure_25dp"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginRight="@dimen/common_measure_10dp"
                            android:layout_marginTop="@dimen/common_measure_10dp"
                            android:src="@drawable/delete_btn" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/prize_region_temp_activity_rule_text1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/common_measure_10dp"
                        android:layout_marginLeft="@dimen/common_measure_10dp"
                        android:layout_marginRight="@dimen/common_measure_10dp"
                        android:layout_marginTop="@dimen/common_measure_25dp"
                        android:lineSpacingMultiplier="1.2"
                        android:text="@string/prize_region_rule1"
                        android:textColor="@color/white"
                        android:textIsSelectable="true"
                        android:textSize="@dimen/common_measure_13dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/prize_region_temp_activity_rule_layout2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/common_measure_247dp"
                    android:layout_marginLeft="@dimen/common_measure_10dp"
                    android:layout_marginRight="@dimen/common_measure_10dp"
                    android:layout_marginTop="@dimen/common_measure_1dp"
                    android:background="@drawable/style_rect_fillet_fulling_deepblue"
                    android:visibility="invisible" >

                    <RelativeLayout
                        android:id="@+id/prize_region_temp_activity_rule1_delbtn2"
                        android:layout_width="@dimen/common_measure_60dp"
                        android:layout_height="@dimen/common_measure_60dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true" >

                        <ImageView
                            android:layout_width="@dimen/common_measure_25dp"
                            android:layout_height="@dimen/common_measure_25dp"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginRight="@dimen/common_measure_10dp"
                            android:layout_marginTop="@dimen/common_measure_10dp"
                            android:src="@drawable/delete_btn" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/prize_region_temp_activity_rule_text2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/common_measure_10dp"
                        android:layout_marginLeft="@dimen/common_measure_10dp"
                        android:layout_marginRight="@dimen/common_measure_10dp"
                        android:layout_marginTop="@dimen/common_measure_25dp"
                        android:gravity="center_vertical"
                        android:lineSpacingMultiplier="1.2"
                        android:text="@string/prize_region_rule2"
                        android:textColor="@color/white"
                        android:textIsSelectable="true"
                        android:textSize="@dimen/common_measure_13dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/prize_region_temp_activity_rule_layout3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/common_measure_247dp"
                    android:layout_marginLeft="@dimen/common_measure_10dp"
                    android:layout_marginRight="@dimen/common_measure_10dp"
                    android:layout_marginTop="@dimen/common_measure_1dp"
                    android:background="@drawable/style_rect_fillet_fulling_deepblue"
                    android:visibility="invisible" >

                    <RelativeLayout
                        android:id="@+id/prize_region_temp_activity_rule1_delbtn3"
                        android:layout_width="@dimen/common_measure_60dp"
                        android:layout_height="@dimen/common_measure_60dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true" >

                        <ImageView
                            android:layout_width="@dimen/common_measure_25dp"
                            android:layout_height="@dimen/common_measure_25dp"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginRight="@dimen/common_measure_10dp"
                            android:layout_marginTop="@dimen/common_measure_10dp"
                            android:src="@drawable/delete_btn" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/prize_region_temp_activity_rule_text3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/common_measure_10dp"
                        android:layout_marginLeft="@dimen/common_measure_10dp"
                        android:layout_marginRight="@dimen/common_measure_10dp"
                        android:layout_marginTop="@dimen/common_measure_25dp"
                        android:gravity="center_vertical"
                        android:lineSpacingMultiplier="1.2"
                        android:text="@string/prize_region_rule3"
                        android:textColor="@color/white"
                        android:textIsSelectable="true"
                        android:textSize="@dimen/common_measure_13dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/prize_region_temp_activity_rule_layout4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/common_measure_247dp"
                    android:layout_marginLeft="@dimen/common_measure_10dp"
                    android:layout_marginRight="@dimen/common_measure_10dp"
                    android:layout_marginTop="@dimen/common_measure_2dp"
                    android:background="@drawable/style_rect_fillet_fulling_deepblue"
                    android:visibility="invisible" >

                    <RelativeLayout
                        android:id="@+id/prize_region_temp_activity_rule1_delbtn4"
                        android:layout_width="@dimen/common_measure_60dp"
                        android:layout_height="@dimen/common_measure_60dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true" >

                        <ImageView
                            android:layout_width="@dimen/common_measure_25dp"
                            android:layout_height="@dimen/common_measure_25dp"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginRight="@dimen/common_measure_10dp"
                            android:layout_marginTop="@dimen/common_measure_10dp"
                            android:src="@drawable/delete_btn" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/prize_region_temp_activity_rule_text4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/common_measure_10dp"
                        android:layout_marginLeft="@dimen/common_measure_10dp"
                        android:layout_marginRight="@dimen/common_measure_10dp"
                        android:layout_marginTop="@dimen/common_measure_25dp"
                        android:gravity="center_vertical"
                        android:lineSpacingMultiplier="1.2"
                        android:text="@string/prize_region_rule4"
                        android:textColor="@color/white"
                        android:textIsSelectable="true"
                        android:textSize="@dimen/common_measure_13dp" />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>

</LinearLayout>